{
    "manifest_version": 3,
    "name": "RFQ Attachment Downloader",
    "version": "1.0",
    "description": "Lists available files and downloads selected ones",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  